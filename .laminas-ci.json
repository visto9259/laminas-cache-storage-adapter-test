{
    "extensions": ["apcu"],
    "ini": ["apc.enabled=1", "apc.enable_cli=1"],
    "additional_checks": [
        {
            "name": "Integration tests",
            "job": {
                "command": "composer require laminas/laminas-cache-storage-adapter-apcu laminas/laminas-cache-storage-adapter-memory && ./vendor/bin/phpunit --testsuite 'Laminas Cache Integration Tests'",
                "php": "*",
                "dependencies": "lowest"
            }
        },
        {
            "name": "Integration tests",
            "job": {
                "command": "composer require laminas/laminas-cache-storage-adapter-apcu laminas/laminas-cache-storage-adapter-memory && ./vendor/bin/phpunit --testsuite 'Laminas Cache Integration Tests'",
                "php": "*",
                "dependencies": "latest"
            }
        },
        {
            "name": "Integration tests",
            "job": {
                "command": "composer require laminas/laminas-cache-storage-adapter-apcu laminas/laminas-cache-storage-adapter-memory && ./vendor/bin/phpunit --testsuite 'Laminas Cache Integration Tests'",
                "php": "@lowest",
                "dependencies": "locked"
            }
        }
    ]
}
